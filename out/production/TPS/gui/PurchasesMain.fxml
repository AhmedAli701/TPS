<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="purchasesVBox" nodeOrientation="RIGHT_TO_LEFT" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.purchases.PurchasesMainController">
    <children>
        <HBox spacing="5.0">
            <children>
                <ComboBox prefHeight="25.0" prefWidth="89.0" promptText="يوم" />
                <ComboBox layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="89.0" promptText="شهر" />
                <ComboBox layoutX="99.0" layoutY="10.0" prefHeight="25.0" prefWidth="89.0" promptText="سنة" />
                <Button mnemonicParsing="false" text="عرض" />
            </children>
        </HBox>
        <TableView VBox.vgrow="ALWAYS">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn editable="false" maxWidth="50.0" minWidth="40.0" prefWidth="50.0" sortable="false" text="رقم" />
                <TableColumn editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="100.0" sortable="false" text="البضاعة" />
                <TableColumn editable="false" maxWidth="60.0" minWidth="60.0" prefWidth="70.0" sortable="false" text="العدد" />
                <TableColumn editable="false" maxWidth="80.0" minWidth="80.0" sortable="false" text="سعر الشراء" />
                <TableColumn editable="false" maxWidth="80.0" minWidth="80.0" sortable="false" text="البائع" />
                <TableColumn editable="false" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" sortable="false" text="التاريخ" />
            </columns>
        </TableView>
        <HBox alignment="CENTER_LEFT" spacing="8.0">
            <children>
                <Label text="إجمالى المشتريات" />
                <TextField editable="false" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</VBox>
